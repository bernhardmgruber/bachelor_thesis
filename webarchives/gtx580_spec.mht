Content-Disposition: inline; filename=main.css
Content-Type: text/css; charset=utf-8; name=main.css
Content-Location: http://www.geforce.com/asset/_includes/css/main.css?vz=2.06
Content-Transfer-Encoding: 8bit

@charset "utf-8";

#fb-root iframe{
    background: none repeat scroll 0 0 transparent !important;
    border: 0 none !important;
    outline: 0 none !important;
    /* IE 7/8 ignores the above and only respects this: */
    display:none;
}
#XdComm{
	display:none;
}


/* HTML TAGS FORMATTING */

body {
    /*background-color:#000;*/
    background-color: #282B30;
    background-image: url(/template/_images/background.jpg); 
    background-repeat:no-repeat;
    background-position:center top;
    color:#eee;
    font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
    font-size:13px;
    margin:0;
    text-align: center; 
}

img {
    border: none;
}
form, input {
    margin:0px;
    padding:0px;
}
.middle input {
    #top:-3px;
}

input.text,
textarea.text,
input.gsc-input{
    border-top:1px solid #000;
    border-left:1px solid #000;
    border-right:1px solid #3c4044;
    border-bottom:1px solid #484c51;
    background-color:#272a2e;
    color:#999;
    position:relative;
    font-size:12px;
    height:20px;
    font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
}
    textarea.text {
        height:auto;    
    }

a {
    text-decoration:underline;
    outline:none;
    color:#76b900;
}
    a:hover {
        color:#fff;
    }
    a.nothing {
        text-decoration:none;
        color:#eee; 
    }
    a:hover.nothing {
        color:#fff;
    }

p, h1, h4, h5, h3 {
    padding-top:0px;
    margin-top:0px;
    padding-bottom:0px;
    margin-bottom:0px;
}

h5 a {
    color: #76b900;
    text-decoration: none;
}

h5 a:hover{
    color: #76b900;
    /*text-decoration: underline;*/
}
h5, h3 {
    font-size:14px;
    font-weight:bold;
    color:#76b900;
    text-shadow:0 1px 0 #000;
}

fieldset {
    border:none;
    padding:0px;
    margin:0px;
}

var {
    display:none;   
}

abbr {
    cursor:pointer;
    border-bottom:dotted 1px;
}

ol li{
    margin-bottom:10px;
}

/* top layer with site lodader */
#siteLoader {
    width:100%;
    height:100%;
    background-image:url(/template/_images/loading_background.png);
    position:fixed;
    z-index:250;
    left: 0px;
    top:0px;
}
    #siteLoader #outer {
        position: absolute;
        top: 50%;
        left: 0px;
        width: 100%;
        height: 1px;
        overflow: visible;
    }

        #siteLoader #outer #inner {
            width: 300px;
            height: 100px;
            margin-left: -150px;  /***  width / 2   ***/
            position: absolute;
            top: -50px;          /***  height / 2   ***/
            left: 50%;
            text-align:center;
            cursor:pointer;
        }



/* MAIN LAYOUT LAYERS */
.contentHolder {
    width:984px;
    position:relative;
    margin-left: auto;
    margin-right: auto;
    text-align:left;
}


#imageControls{  }

.imageContainerHolder{
  background-color:#181C21;
  height:415px;
  line-height:415px;
  overflow:hidden;
  position:relative;
  text-align:center;
  width:652px;
}

#imageContainer {
  cursor:pointer;
  display:table-cell;
  height:415px;
  overflow:hidden;
  width:652px;
}

#imageContainer a {
  margin:0 auto;
}

#imageContainer img {
  vertical-align:middle;
}

.header {
    position:relative;
    height:190px;
    width:984px;
    z-index:10;
}
    .header .logoRow{
        position:relative;
        height:100px;
        width:984px;
        z-index:20;
    }
        .header .logoRow .geforceLogo{
            position:absolute;
            height:55px;
            width:218px;
            left:10px;
            top:15px;
        }
        
        /* MENU WITH COUNTRY, SUPPORT AND SEARCH  + 31px width cmp*/
        .header .logoRow .rightMenu{
            position:absolute;
            height:29px;
            width:385px;
            left:600px;
            top:25px;
            background-image:url(/template/_images/header_logoRow_background.png);
        }   
             
            .header .logoRow .rightMenu .support,
            .header .logoRow .rightMenu .searchBox {
                position:relative;
                float:left;
                font-size:11px;
                color:#998;
            }
            

            #countriesCollapsed {
                position:absolute;
                top:1px;
                left:1px;
                width:100%;
                height:27px;
                line-height:26px;
                padding-left:10px;
                cursor:pointer;
            }   
                #countriesCollapsed:hover {
                    color:#76b900;
                }
                #countriesCollapsed .arrow{
                    padding:5px;
                    top:2px;
                    position:absolute;
                    left:120px;
                    cursor:pointer;
                }
                
                #countriesCollapsed .flag{
                    padding: 0px 5px 3px 0px;
                    vertical-align:middle;
                }
                
                #countriesCollapsed .overlay {
                    
                    background-repeat:no-repeat;
                    position:absolute;
                    width:95px;
                    height:23px;
                    left:1px;
                    top:1px;
                    display:none;
